<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_matrimonio_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pe.gob.munihuacho.munimovil.activities.MatrimonioDetailActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4.5">

        <TextView
            android:text="@string/esposo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/textView66"
            android:gravity="bottom|center_vertical"
            android:textStyle="normal|bold"
            android:paddingLeft="3dp"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            android:paddingRight="3dp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/apepaterno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:id="@+id/textView63"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvPaternoDonMat" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/apematerno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:layout_weight="1"
                android:id="@+id/textView65" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvMaternoDonMat" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/nombres"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:layout_weight="1"
                android:id="@+id/textView67" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvNombresDonMat" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4.5"
        android:gravity="bottom|center_vertical">

        <TextView
            android:text="@string/esposa"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/textView64"
            android:layout_weight="1"
            android:textStyle="normal|bold"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp"
            android:gravity="bottom|left" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/apepaterno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:id="@+id/textView21314"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvPaternoMat" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/apematerno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:layout_weight="1"
                android:id="@+id/textView6513" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvMaternoMat" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/nombres"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:layout_weight="1"
                android:id="@+id/textView234" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvNombresMat" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/fechaMatrimonio"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:id="@+id/textView70"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvFechMat" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:text="@string/fechainscripcion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtrazon"
                android:id="@+id/textView74"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/muni.forms.txtAnswer"
                android:id="@+id/tvInscripMat" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
